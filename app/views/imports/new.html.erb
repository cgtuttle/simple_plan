<h5>Import File</h5>

  <%= form_for @import, :html=>{:multipart=>true} do |f| %>
    <p> Select a CSV File :</p>
    <p><%= file_field_tag :file %></p>
    <p>Import into <%= f.select :model, Import::MODELS.sort %></p> 
    <%= submit_tag 'Continue', :name => 'import_file' %>
    <%= submit_tag 'Cancel', :name => 'import_file' %>
  <% end %>
  
  