

<h2><%=  @title %></h2>
<h4>Plan: <%= @plan.code %></h4>
<%#= paginate @sellers %>

<div id="table_container">

  <table>
    <thead>
    <tr class="row-header">
      <th>Rep</th>
      <th>Program</th>
      <th>Program Start</th>
      <th>Program End</th>
      <th>Deal</th>
      <th>Customer</th>
      <th>Product</th>
      <th>Activity</th>
    </tr>
    </thead>

    <tbody class="scrolling">
      <% @sellers.each do |s| %>

        <tr>
          <td><%= s.name %></td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <%= render s.ordered_seller_programs %>
        </tr>

      <% end %>
    </tbody>

  </table>
</div>


